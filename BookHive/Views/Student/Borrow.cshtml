@{

    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Book Borrow List";

}
@model List<BookiHive.Model.CartViewModel>

    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div style="overflow: auto;">
                            <h4 class="card-title" style="text-align:center;justify-content:center;">Borrowed List</h4>
                        </div>
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered zero-configuration">
                                <thead>
                                    <tr>
                                        <th>Book Name</th>
                                    <th>Author Name</th>
                                        <th>Faculty</th>
                                        <th>Semister</th>
                                        <th>Year</th>
                                        <th>Price</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>

                                    @foreach (var item in Model)
                                    {
                                        <tr>

                                            <td>@item.BookName</td>
                                        <td>@item.Name</td>
                                            <td>@item.Faculty</td>
                                            <td>@item.Semister</td>
                                            <td>@item.Year</td>
                                            <td>@item.Price</td>

                                           
                                        </tr>
                                    }

                                </tbody>
                                <tfoot>
                                </tfoot>
                            </table>
                       
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
