@{
    Layout = "~/Views/Shared/_Layout.cshtml";

}

@model RegisterList


<div class="login-form-bg h-150" style="margin-top:50px">
    <div class="container h-100">
        <div class="row justify-content-center h-100">
            <div class="col-xl-6">
                <div class="form-input-content">
                    <div class="card login-form mb-0">

                        <div class="card-body pt-5">
                            <div class="form-validation">

                                <div class="basic-form">
                                    <form id="Deatils" method="post" class="form-valide">
                                        <h4> Student Deatils</h4>
                                        <hr />

                                        <div class="form-group row">
                                            <label class="col-lg-12 col-form-label" for="val-email">
                                                Email 
                                            </label>
                                            <div class="col-lg-12">
                                                <input type="email" class="form-control" id="val-email" value="@Model.email" name="email" placeholder="Enter email.." readonly>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-lg-12 col-form-label" for="val-username">
                                                User Name 
                                            </label>
                                            <div class="col-lg-12">
                                                <input type="text" class="form-control" id="val-username" value="@Model.user_name" name="username" placeholder="Enter code no" readonly>
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <label class="col-lg-12 col-form-label" for="val-email">
                                                Email 
                                            </label>
                                            <div class="col-lg-12">
                                                <input type="email" class="form-control" id="val-email" value="@Model.email" name="email" readonly>
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <label class="col-lg-12 col-form-label" for="val-codeno">
                                                Code No 
                                            </label>
                                            <div class="col-lg-12">
                                                <input type="text" class="form-control" id="val-codeno" value="@Model.code" name="code" placeholder="Enter code no" readonly>
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <label class="col-lg-12 col-form-label" for="val-image">
                                                Qr Image
                                            </label>
                                            <div class="col-lg-12">
                                                <img src="@Url.Content(Model.filename)" style="width:100px;height:100px;" alt="qrcode" />
                                            </div>
                                        </div>
                                       
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
